# hacky thing I had to do: all lines that were more than one line would require 2 clicks to advance subtitles, I added an extra \n to the end of those lines and the subtitles advance as expected now.

extends RichTextLabel
var text_box_size
var current_scroll_position = 0
var line_count

func _ready():
	print("captions =",Globals.captions)
	self.bbcode_text = " "
	if Globals.captions == true:
		self.visible = true
		print("caps visible: ", self.visible)
	else:
		self.visible = false
	text_box_size = self.rect_size
	self.set("theme_override_constants/line_separation", -50)

func _on_mercs_audio_player_soundEmitted(line):
	current_scroll_position = 1
	print("line = ", line)
	self.set("bbcode_text", captions[int(line)])
	line_count = self.bbcode_text.count('\n') + 1
	print("lines =", line_count)
	if self.get_line_count() > 1:
		get_node("advance_text_button").visible = true
	else:
		get_node("advance_text_button").visible = false

func _on_advance_text_button_pressed():
	print(current_scroll_position)
	line_count = self.bbcode_text.count('\n') + 1
	if current_scroll_position < line_count - 1: 
		current_scroll_position += 1
		self.scroll_to_line(current_scroll_position)
	else:
		self.scroll_to_line(0)
		current_scroll_position = 0





























































